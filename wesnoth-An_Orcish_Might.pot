msgstr ""

#. [message]: speaker=Tragtrad
#: An_Orcish_Might/scenarios/02_The_Ford.cfg:1362
msgid "We need to flee, or we will die!"
msgstr ""

#. [message]: speaker=Umaduro
#: An_Orcish_Might/scenarios/02_The_Ford.cfg:1366
msgid "Only if Rormuk is dead."
msgstr ""

#. [message]: role=cowards
#: An_Orcish_Might/scenarios/02_The_Ford.cfg:1424
